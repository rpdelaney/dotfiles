{
    "cookies": "/tmp/cookies.txt",
    "filename": {
        "len(f\"{title} + '.' + {extension}.part\") > 254": "f\"{id}_{title.encode('utf-8')[:-200].decode('utf-8')}...{extension}\"",
        "not locals().get('title')": "{id}.{extension}",
        "": "{id}_{title}.{extension}"
    },
    "postprocessors": [
        {
            "name": "metadata"
        }
    ],
    "ytdl": {
        "enabled": true,
        "module": "yt_dlp"
    },
    "extractor": {
        "4chan": {
          "directory": "{title}_{num}",
          "filename": "{filename}_{timestamp}.{extension}"
        },
        "instagram": {
            "directory": "{author}",
            "filename": "{media_id}_{shortcode}.{extension}",
            "videos": true,
            "sleep-request": [15, 45],
            "sleep": [2, 10],
            "include": "stories"
        },
        "deviantart": {
            "#": "download 'gallery' and 'scraps' images for user profile URLs",
            "include": "gallery,scraps"
        },
        "reddit": {
            "directory": "{author}",
            "parent-directory": true,
            "parent-metadata": "_reddit_"
        },
        "tumblr":
        {
            "avatar": true
        },
        "imgur": {
            "mp4": "always"
        },
        "redgifs":
        {
            "format": ["hd", "sd", "gif"]
        },
        "gfycat": {
            "format": ["webm", "mp4", "mobile", "gif"]
        }
    }
}
